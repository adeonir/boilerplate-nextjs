{
  "presets": ["next/babel", "@babel/preset-typescript"],
  "plugins": [
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": false
      }
    ]
  ],
  "env": {
    "development": {
      "plugins": [
        [
          "styled-components",
          {
            "ssr": true,
            "displayName": true
          }
        ]
      ]
    },
    "test": {
      "plugins": [
        [
          "styled-components",
          {
            "ssr": false,
            "displayName": false
          }
        ]
      ]
    }
  }
}
